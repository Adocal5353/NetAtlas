@model NetAtlas.Models.PhotoVideo

@{
    ViewData["title"] = "Poster un media";
    Layout = "_LayoutMembre";
}


<h4>PhotoVideo</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <h3 class="text-danger">@ViewBag.Message</h3>
            <div class="form-group">
                <label asp-for="nomRessource" class="control-label"></label>
                <input asp-for="nomRessource" class="form-control" />
                <span asp-validation-for="nomRessource" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label for="image" class="control-label"></label>
                <input  class="form-control" name="image" type="file" id="image" />
                <span asp-validation-for="TailleEnMo" class="text-danger"></span>
            </div>
            
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}

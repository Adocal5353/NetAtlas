@model IEnumerable<NetAtlas.Models.Membre>

@{
    ViewData["Title"] = "ListMembre";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h1>ListMembre</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nom)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Prenom)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            
            <th>
                @Html.DisplayNameFor(model => model.NbrAvertissement)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.isLogged)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nom)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Prenom)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
           
            <td>
                @Html.DisplayFor(modelItem => item.NbrAvertissement)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.isLogged)
            </td>
            <!--<td>
                <a asp-action="Edit" asp-route-id="@item.Id">Supprimer</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>-->
        </tr>
}
    </tbody>
</table>
